{
    "name": "efdi/local-multiselect-filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "rcknr/nova-multiselect-filter": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Efdi\\LocalMultiselectFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Efdi\\LocalMultiselectFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
