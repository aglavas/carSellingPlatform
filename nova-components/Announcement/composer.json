{
    "name": "efdi/announcement",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Efdi\\Announcement\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Efdi\\Announcement\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
